        <input type="text" placeholder="Roll Number" name="rollNumber" id="rollNumber" required>
        </fieldset>
        <h2>First Name </h2>
        <fieldset> 
        <input type="text" placeholder="First Name" name="first_name" id="first_name" required>      
        </fieldset>
        <h2>Last Name </h2>
        <input type="text" placeholder="Last Name" name="last_name" id="last_name" required>   
        </fieldset>
		<h2>Password </h2>
        <fieldset>   
        <input type="password" placeholder="Password" name="password" id="password" required>  
        </fieldset>
		<h2>Email Address</h2>
        <fieldset>   
        <input type="email" placeholder="Email Address" name="email_address" id="email_address" required>   
        </fieldset>
        <h2>Semester</h2>
        <fieldset>       
        <input type="number" placeholder="semester" name="semester" id="semester" required>   
        </fieldset>
        <h2>GPA</h2>    
        <fieldset>
        <input type="number"  placeholder="GPA" name="GPA" id="GPA" step=".01" required>   
        </fieldset>
		<h2>Gender</h2>     
        </b>    
        </label>
		<input type="radio" value="male" name="gender" id="male" required><label>Male</label><br>
		<input type="radio" value="female" name="gender" id="female" required><label>Female</label><br>
        <input type="radio" value="others" name="gender" id="others" required><label>Others</label><br>
		</center>
        <fieldset>
        <button type="submit" id="log" onclick="return confirmSubmit()">Student Register!</button>       
        </fieldset>
    </form>     
</div>   
  </div>
</body>
</div>
</html> 